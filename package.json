{
	"name": "webcolors-cmyk",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "https://github.com/danburzo/webcolors-cmyk.git",
	"author": "Dan Burzo <danburzo@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@zeit/next-css": "^1.0.1",
		"culori": "^0.10.1",
		"ejs": "^2.5.7",
		"next": "^8.1.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6"
	},
	"devDependencies": {
		"fast-glob": "^3.0.1",
		"gh-pages": "^2.0.1",
		"husky": "^2.4.1",
		"icc": "^1.0.0",
		"node-lcms": "^1.0.2",
		"prettier": "1.18.2",
		"pretty-quick": "^1.11.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "none",
		"useTabs": true,
		"tabWidth": 4,
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"printWidth": 80
	},
	"scripts": {
		"dev": "next",
		"build": "next build",
		"start": "next start",
		"export": "next export -o dist",
		"deploy": "rm -rf .next && yarn build && yarn export && gh-pages -d dist --dotfiles"
	}
}
